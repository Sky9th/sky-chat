    <Style src="project://database/Assets/UI/Chat.uss?fileID=7433441132597879392&amp;guid=2b3e98dc3c4477e409b379624c3c865f&amp;type=3#Chat" />
    <ui:GroupBox name="ChatContainer" class="container" style="visibility: visible; display: flex;">
        <ui:VisualElement name="ChatPanel" class="panel" style="font-size: 25%; padding-left: 16px; padding-right: 16px; padding-top: 16px; padding-bottom: 16px; border-top-left-radius: 5px; border-bottom-left-radius: 5px; border-top-right-radius: 5px; border-bottom-right-radius: 5px; bottom: 5%; left: 1%;">
            <ui:ScrollView name="ChatList" mode="Vertical" focusable="true" horizontal-scroller-visibility="Hidden" vertical-scroller-visibility="Hidden" class="chatList" style="padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0;" />
