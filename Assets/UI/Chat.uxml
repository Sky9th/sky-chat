                <ui:TextField picking-mode="Ignore" name="ChatInput" readonly="false" focusable="false" class="ChatInput" style="border-top-left-radius: 5px; border-bottom-left-radius: 5px; border-top-right-radius: 5px; border-bottom-right-radius: 5px; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; color: rgb(53, 53, 53); font-size: 38%; background-color: rgb(207, 207, 207);" />
                <ui:Button text="Send" display-tooltip-when-elided="true" name="ChatBtn" focusable="false" class="ChatBtn" style="font-size: 15%; border-top-left-radius: 5px; border-bottom-left-radius: 5px; border-top-right-radius: 5px; border-bottom-right-radius: 5px; margin-left: auto; margin-right: 0; margin-top: 0; margin-bottom: 0;" />
